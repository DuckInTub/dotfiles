[
  {
    "label": "Lock",
    "exec": "hyprlock",
    "icon": "./images/system-lock-screen.svg"
  },
  {
    "label": "Logout",
    "exec": "swaymsg exit",
    "icon": "./images/system-log-out.svg"
  },
  {
    "label": "Sleep",
    "exec": "systemctl sleep",
    "icon": "./images/system-suspend.svg"
  },
  {
    "label": "Reboot",
    "exec": "systemctl reboot",
    "icon": "./images/system-reboot.svg"
  },
  {
    "label": "Shutdown",
    "exec": "systemctl -i poweroff",
    "icon": "./images/system-shutdown.svg"
  }
]
